<div id="grid">
    <% for (let release of releases) { %>
    <a href="/release/<%= release.id %>"><div class="card">
        <img src="https://placekitten.com/250/250">
        <div class="info">
            <h3><%= release.title %></h2>
            <h4><%= release.artist %></h3>
            <h4><%= release.catalog_number %></h4>
        </div>
    </div></a>
    <% } %>
</div>

