<form action="/release/<%= release.id %>?_method=PUT" method="post" enctype="multipart/form-data">
    <!-- <input type="hidden" name="_method" value="put"> -->
    <label for="">Title</label>
    <input type="text" name="title" value="<%= release.title %>" required>
    <label for="">Artist</label>
    <input type="text" name="artist" value="<%= release.artist %>" required>
    <label for="">Genre</label>
    <input type="text" name="genre" value="<%= release.genre %>">
    <label for="">Catalog#</label>
    <input type="text" name="catalog_number" value="<%= release.catalog_number %>" required>
    <label for="">Album Art</label>
    <input type="file" name="album_art">
    <button>Submit</button>
</form>