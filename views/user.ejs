<h2><%= user.username %></h2>
<h3><%= user.location %></h3>
<h4>About Me:</h4>
<p><%= user.bio %></p>
<img src="https://placekitten.com/250/250">

<% if (isCurrentUserPage()) { %>
<a href="/user/<%= user.id %>/edit">Edit</a>
<form action="/user/<%= user.id %>" method="post">
    <input type="hidden" name="_method" value="delete">
    <button>Delete</button>
</form>
<% } %>